{
  "name": "api_og",
  "version": "0.0.1",
  "private": true,
  "main": "api/serverless.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsc -p tsconfig.json && node api/serverless.js",
    "build": "tsc -p tsconfig.json",
    "start": "node api/serverless.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 ../../node_modules/canvas/build/Release/ && yarn build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "canvas": "^2.11.0",
    "cross-fetch": "^3.1.5",
    "fastify": "^4.11.0",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.9",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "typescript": "^4.9.4"
  }
}
